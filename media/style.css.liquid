/* setting the width and height of the SELECT element to match the replacing graphics */
select.select{
    position:relative;
    z-index:10;
    width:90px !important;
    height:20px !important;
    line-height:20px;
}
/* dynamically created SPAN, placed below the SELECT */
span.select{
    position:absolute;
    bottom:0;
    float:left;
    left:0;
    width:90px;
    height:20px;
    line-height:20px;
    text-indent:10px;
  background:url({{ 'select.svg.png' | asset_url }}) no-repeat scroll 93% 55% {{settings.padd_color2}};
